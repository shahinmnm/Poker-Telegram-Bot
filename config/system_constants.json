{
  "default_webhook_listen": "127.0.0.1",
  "default_webhook_port": 3000,
  "default_webhook_path": "/telegram/webhook-poker2025",
  "default_rate_limit_per_second": 1,
  "default_rate_limit_per_minute": 20,
  "default_timezone_name": "Asia/Tehran",
  "locks": {
    "category_timeouts_seconds": {
      "engine_stage": 25.0,
      "engine_stage_betting": 30.0
    },
    "action": {
      "ttl": 10,
      "valid_types": ["fold", "check", "call", "raise"]
    },
    "retry_strategy": {
      "max_retries": 3,
      "initial_backoff_seconds": 0.5,
      "backoff_multiplier": 1.5,
      "total_timeout_seconds": 10.0,
      "enable_queue_estimation": true
    }
  },
  "lock_manager": {
    "enable_fine_grained_locks": false,
    "fine_grained_rollout_percentage": 0
  },
  "stats_batch_buffer": {
    "max_size": 100,
    "flush_interval_seconds": 5,
    "flush_on_shutdown": true,
    "enable_metrics": true,
    "max_retries": 3,
    "retry_backoff_base": 2
  }
}
